  "scripts": {
    "dev": "vue-cli-service serve",
    "build": "vue-cli-service build",
    "preview": "node build/index.js --preview",
    "svgo": "svgo -f src/icons/svg --config=src/icons/svgo.yml",
    "lint": "eslint --ext .js,.vue src",
    "test:unit": "jest --clearCache && vue-cli-service test:unit",
    "swagger": "swagger-builder -f=./public/static/swagger/swagger.yaml -o=./src/api/apis.js",
    "test:ci": "npm run lint && npm run test:unit",
    "new": "plop",
    "docker:build": "sudo docker build -t registry.cn-beijing.aliyuncs.com/yushk/sport_web .",
    "docker:run": "sudo docker run --rm --name sport_web -d -p 7003:80 yushk/sport_web",
    "docker": "npm run build && sudo docker build -t registry.cn-beijing.aliyuncs.com/yushk/sport_web . && sudo docker push registry.cn-beijing.aliyuncs.com/yushk/sport_web",
